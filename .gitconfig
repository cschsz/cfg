[user]
      email = c@sch-sz.de
      name = cschsz
[alias]
      br = branch
      cm = commit -m
      go = checkout
      ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
      ls = log --pretty=format:'%C(auto)%<(8)%h %C(auto)%<(28)%ci %C(bold blue)%<(16,trunc)%cn %C(reset)%s %C(dim white)%d'
      s = !git branch && git status
      tree = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative

